  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saberzero1/quartz-themes/issues"
  },
  "homepage": "https://github.com/saberzero1/quartz-themes#readme",
  "devDependencies": {
    "prettier": "^3.3.3",
    "sass": "^1.79.3"
  },
  "dependencies": {
    "just-install": "^2.0.2"
  }
